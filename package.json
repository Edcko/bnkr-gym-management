{
  "name": "bnkr-gym-management",
  "version": "1.0.0",
  "description": "Sistema de gestiÃ³n para gimnasio BNKR",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup:server": "chmod +x deploy-server.sh && chmod +x setup-project.sh",
    "deploy:server": "./deploy-server.sh",
    "setup:project": "./setup-project.sh"
  },
  "keywords": [
    "gym",
    "management",
    "vue",
    "nodejs",
    "postgresql",
    "prisma"
  ],
  "author": "BNKR Gym",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 